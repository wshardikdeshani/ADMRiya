                            url: '/ADMHeader/Save',
                            type: "POST",
                            contentType: false, // Not to set any content header
                            processData: false, // Not to process data
                            data: fileData,
                            success: function (mRes) {
                                if (mRes == 'Ticket already added in ADM.' || mRes == 'ADM already raised.') {
                                    toastr.error(mRes);
                                }
                                else {
                                    toastr.success(mRes);

                                    ClearControls();

                                    $("#ADMDefault").modal('hide');

                                    GetData(10, 1, '');
                                }
                            },
                            error: function (err) {
                                toastr.success(err.statusText);
                            }
                        });
                    }
                    else {
                        toastr.success('FormData is not supported.');
                    }
                    return false;
                }
            });
        }

        function DisplayDebitOption() {
            var StatusIDF = $("#StatusIDF").val();

            if (StatusIDF == 5 || StatusIDF == 10) // Agent
            {
                $("#DebitToRiya").hide();
                $("#DebitToSubAgent").show();
                $("#divFUACM").hide();
            }
            else if (StatusIDF == 4 || StatusIDF == 9) // Riya
            {
                $("#DebitToRiya").show();
                $("#DebitToSubAgent").hide();
                $("#divFUACM").hide();
            }
            else if (StatusIDF == 7) // ACM Received
            {
                $("#DebitToRiya").hide();
                $("#DebitToSubAgent").hide();
                $("#divFUACM").show();
            }
        }

        function DisplayDebitToOption() {
            var DebitToType = $("#DebitToType").val();

            if (DebitToType == 1) { // Staff
                $("#DebitTo").val("");
                $("#divDebitToRiya").show();
            }
            else if (DebitToType == 2) { // Riya
                $("#DebitTo").val("Company Account");
                $("#divDebitToRiya").hide();
            }
        }
    </script>
}